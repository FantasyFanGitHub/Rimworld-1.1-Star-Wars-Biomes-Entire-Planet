        <diseaseInc>Disease_AnimalFlu</diseaseInc>
        <commonality>100</commonality>
      </li>
      <li>
        <diseaseInc>Disease_AnimalPlague</diseaseInc>
        <commonality>100</commonality>
      </li>
    </diseases>
    <terrainsByFertility>
      <li>
        <terrain>ScarifWater</terrain>
        <min>-999</min>
        <max>0.87</max>
      </li>
      <li>
        <terrain>Soil</terrain>
        <min>0.87</min>
        <max>999</max>
      </li>
    </terrainsByFertility>
    <terrainPatchMakers>
      <li>
        <perlinFrequency>0.015</perlinFrequency>
        <thresholds>
          <li>
            <terrain>SoilRich</terrain>
            <min>0.73</min>
            <max>0.80</max>
          </li>
          <li>
            <terrain>Sand</terrain>
            <min>0.80</min>
            <max>0.93</max>
          </li>
        </thresholds>
      </li>
    </terrainPatchMakers>
    <baseWeatherCommonalities>
      <Clear>18</Clear>
      <Fog>1</Fog>
      <Rain>2</Rain>
      <DryThunderstorm>0.3</DryThunderstorm>
      <RainyThunderstorm>1.7</RainyThunderstorm>
      <FoggyRain>1</FoggyRain>
      <SnowGentle>4</SnowGentle>
      <SnowHard>4</SnowHard>
    </baseWeatherCommonalities>
    <wildPlants>
      <Plant_Grass>3.0</Plant_Grass>
      <Plant_TallGrass>2.0</Plant_TallGrass>
      <Plant_ShrubLow>2.0</Plant_ShrubLow>
