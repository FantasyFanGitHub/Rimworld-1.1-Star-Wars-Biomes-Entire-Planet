      <Alpaca>0.5</Alpaca>
      <Cobra>0.15</Cobra>
      <Panther>0.15</Panther>
    </wildAnimals>
    <allowedPackAnimals>
      <li>Muffalo</li>
      <li>Elephant</li>
      <li>Alpaca</li>
    </allowedPackAnimals>
  </BiomeDef>

