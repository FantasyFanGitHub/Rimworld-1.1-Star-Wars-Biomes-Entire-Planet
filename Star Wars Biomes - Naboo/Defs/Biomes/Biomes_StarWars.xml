      <Wolf_Timber>0.07</Wolf_Timber>
      <Fox_Red>0.07</Fox_Red>
      <Lynx>0.07</Lynx>
    </wildAnimals>
    <allowedPackAnimals>
      <li>Muffalo</li>
      <li>Alpaca</li>
    </allowedPackAnimals>
  </BiomeDef>

